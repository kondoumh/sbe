{
  "name": "sbe",
  "version": "2.7.0-beta.2",
  "description": "An unofficial Scrapbox desktop app by Electron",
  "main": "src/main.js",
  "config": {
    "proxy": "your.proxy.host:port"
  },
  "build": {
    "productName": "sbe",
    "appId": "sbe",
    "mac": {
      "icon": "icons/mac/sbe.icns"
    },
    "win": {
      "icon": "icons/win/sbe.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "trace": "electron --trace-warnings .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "https://github.com/kondoumh/sbe",
  "keywords": [
    "Electron",
    "Scrapbox"
  ],
  "author": "kondoumh",
  "license": "MIT",
  "devDependencies": {
    "braces": ">=2.3.1",
    "cpx": "^1.5.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "jest": "^27.5.1",
    "minimist": ">=1.2.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "compare-versions": "^4.1.3",
    "dragula": "^3.7.3",
    "electron-context-menu": "^3.1.1",
    "electron-search-text": "^0.3.0",
    "electron-store": "^8.0.1",
    "electron-tabs": "^0.15.0",
    "glob-parent": ">=5.1.2",
    "node-fetch": "^2.6.7"
  }
}
