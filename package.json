{
  "name": "sbe",
  "version": "0.1.0",
  "description": "Scrapbox in Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-macos": "electron-packager . sbe --platform=darwin --arch=x64 --overwrite --icon=icons/mac/sbe.icns",
    "package-win32": "electron-packager . sbe --platform=win32 --arch=x64 --overwrite --icon=icons/win/sbe.ico"
  },
  "repository": "https://github.com/kondoumh/sbe",
  "keywords": [
    "Electron",
    "Scrapbox"
  ],
  "author": "kondoumh",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.8",
    "electron-packager": "^12.0.2"
  },
  "dependencies": {
    "electron-context-menu": "^0.10.1",
    "electron-search-text": "^0.3.0",
    "electron-tabs": "^0.9.5"
  }
}
