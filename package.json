{
  "name": "sbe",
  "version": "1.0.2",
  "description": "An unofficial Scrapbox desktop app by Electron",
  "main": "src/main.js",
  "config": {
    "proxy": "your.proxy.host:port"
  },
  "scripts": {
    "start": "electron .",
    "proxy": "electron . --proxy-server=%npm_package_config_proxy%",
    "package-macos": "electron-packager . sbe --platform=darwin --arch=x64 --overwrite --icon=icons/mac/sbe.icns",
    "package-win32": "electron-packager . sbe --platform=win32 --arch=x64 --overwrite --icon=icons/win/sbe.ico"
  },
  "repository": "https://github.com/kondoumh/sbe",
  "keywords": [
    "Electron",
    "Scrapbox"
  ],
  "author": "kondoumh",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.0.9",
    "electron-packager": "^14.0.6"
  },
  "dependencies": {
    "about-window": "^1.13.1",
    "dragula": "^3.7.2",
    "electron-context-menu": "^0.15.0",
    "electron-search-text": "^0.3.0",
    "electron-store": "^4.0.0",
    "electron-tabs": "^0.9.5"
  }
}
