{
  "name": "sbe",
  "version": "1.5.1",
  "description": "An unofficial Scrapbox desktop app by Electron",
  "main": "src/main.js",
  "config": {
    "proxy": "your.proxy.host:port"
  },
  "build": {
    "productName": "sbe",
    "appId": "sbe",
    "mac": {
      "icon": "icons/mac/sbe.icns"
    },
    "win": {
      "icon": "icons/win/sbe.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "node test/test-start.js"
  },
  "repository": "https://github.com/kondoumh/sbe",
  "keywords": [
    "Electron",
    "Scrapbox"
  ],
  "author": "kondoumh",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "spectron": "^10.0.1"
  },
  "dependencies": {
    "about-window": "^1.13.2",
    "dragula": "^3.7.2",
    "electron-context-menu": "^0.16.0",
    "electron-search-text": "^0.3.0",
    "electron-store": "^5.1.0",
    "electron-tabs": "^0.10.0",
    "node-fetch": "^2.6.0"
  }
}
